<?xml version="1.0" encoding="UTF-8"?>
<schematable:SchemaDeTable
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:schematable="http://schematable"
    nom="toto"
    colonneLignes="//@colonnes.0">
  <colonnes
      xsi:type="schematable:ColonneBrute"
      nom="lignes"
      identifiant="toto.lignes"/>
  <colonnes
      xsi:type="schematable:ColonneCalculee"
      nom="col2"
      identifiant="toto.col2">
    <algorithme
        href="algo_test.xmi#/"/>
    <identifiantsColonnesEntree>toto.lignes</identifiantsColonnesEntree>
  </colonnes>
  <colonnes
      xsi:type="schematable:ColonneBrute"
      nom="col1"
      identifiant="toto.col1"/>
  <contraintes>
    <algorithme
        href="contrainte_test.xmi#/"/>
    <identifiantsColonnesEntree>toto.lignes</identifiantsColonnesEntree>
    <identifiantsColonnesEntree>toto.col2</identifiantsColonnesEntree>
  </contraintes>
</schematable:SchemaDeTable>
