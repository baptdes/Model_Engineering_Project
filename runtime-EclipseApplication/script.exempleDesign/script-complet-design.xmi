<?xml version="1.0" encoding="UTF-8"?>
<script:Script
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:script="http://www.example.org/script">
  <arguments
      nom="dansCos">
    <sortie
        entree="//@blocs.1/@entree"
        nom="arg"/>
  </arguments>
  <resultat
      nom="Res">
    <entree
        sortie="//@blocs.2/@sortie"/>
  </resultat>
  <blocs xsi:type="script:Constante"
      valeur="4.0">
    <sortie
        entree="//@blocs.2/@entree.1"
        nom="const"/>
  </blocs>
  <blocs xsi:type="script:FonctionUnaire"
      fonction="Cosinus">
    <entree
        sortie="//@arguments.0/@sortie"/>
    <sortie
        entree="//@blocs.2/@entree.0"
        nom="cos"/>
  </blocs>
  <blocs xsi:type="script:OperationBinaire">
    <entree
        sortie="//@blocs.1/@sortie"/>
    <entree
        sortie="//@blocs.0/@sortie"/>
    <sortie
        entree="//@resultat.0/@entree"
        nom="add"/>
  </blocs>
</script:Script>
