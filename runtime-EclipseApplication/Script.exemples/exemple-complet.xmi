<?xml version="1.0" encoding="UTF-8"?>
<script:Script
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:script="http://www.example.org/script"
    nom="sommeCosSin">
  <arguments
      nom="argcos">
    <sortie
        entree="//@blocs.0/@entree"
        nom="arg-cos"/>
  </arguments>
  <arguments
      nom="argsin">
    <sortie
        entree="//@blocs.1/@entree"
        nom="arg-sin"/>
  </arguments>
  <resultat
      nom="res">
    <entree
        sortie="//@blocs.2/@sortie"/>
  </resultat>
  <blocs xsi:type="script:FonctionUnaire"
      fonction="Cosinus">
    <entree
        sortie="//@arguments.0/@sortie"/>
    <sortie
        entree="//@blocs.2/@entree.0"
        nom="cos"/>
  </blocs>
  <blocs xsi:type="script:FonctionUnaire"
      fonction="Sinus">
    <entree
        sortie="//@arguments.1/@sortie"/>
    <sortie
        entree="//@blocs.2/@entree.1"
        nom="sin"/>
  </blocs>
  <blocs xsi:type="script:OperationBinaire">
    <entree
        sortie="//@blocs.0/@sortie"/>
    <entree
        sortie="//@blocs.1/@sortie"/>
    <sortie
        entree="//@resultat.0/@entree"
        nom="resultat"/>
  </blocs>
</script:Script>
